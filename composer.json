{
    "name": "dotpulse/outdatedbrowser",
    "type": "typo3-flow-plugin",
    "version": "0.2.0",
    "license": "MIT",
    "description": "Outdated Browser for Neos CMS",
    "foundation": {
        "name": "dotpulse AG",
        "url": "http://dotpulse.ch/"
    },
    "keywords": ["flow", "neos", "Outdated Browser"],
    "authors": [
        {
            "name": "Jon Uhlmann",
            "email": "me@jonnitto.ch",
            "homepage": "https://twitter.com/jonnitto",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/neos": "2.*"
    },
    "autoload": {
        "psr-0": {
            "Dotpulse\\OutdatedBrowser": "Classes"
        }
    },
    "archive": {
        "exclude": [".editorconfig", ".gitignore", ".github", ".gitattributes", "package.json", "Banner.js"]
    },
    "extra": {
        "marketplace-account": "neos@dotpulse.ch",
        "installer-name": "Dotpulse.OutdatedBrowser",
        "neos": {
            "package-key": "Dotpulse.OutdatedBrowser"
        }
    }
}
