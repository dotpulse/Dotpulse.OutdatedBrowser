prototype(Dotpulse.OutdatedBrowser:CSS) <  prototype(TYPO3.TypoScript:Tag) {
	@context.settings = ${Configuration.setting('Dotpulse.OutdatedBrowser')}
	tagName = 'link'
	attributes {
		rel = 'stylesheet'
		href = TYPO3.TypoScript:ResourceUri {
			path = 'resource://Dotpulse.OutdatedBrowser/Public/Main.css'
		}
	}
	@if.includeCSS = ${settings.includeCSS}
	@position = 'end 5000'
}
