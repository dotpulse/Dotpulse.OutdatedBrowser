prototype(Dotpulse.OutdatedBrowser:CSS) <  prototype(TYPO3.TypoScript:Case) {
    inline = ${Configuration.setting('Dotpulse.OutdatedBrowser.css.inline')}
    path = 'resource://Dotpulse.OutdatedBrowser/Public/Main.css'

    @context {
        inline = ${this.inline}
        path = ${this.path}
    }
    @ignoreProperties = ${['inline','path']}

    inlineFile {
        condition = ${inline ? true : false}
        renderer = TYPO3.TypoScript:Tag {
            tagName = 'style'
            content = ${Carbon.FileContent.path(path)}
        }
    }
    externalFile {
        condition = true
        renderer = TYPO3.TypoScript:Tag {
            tagName = 'link'
            attributes {
                rel = 'stylesheet'
                href = TYPO3.TypoScript:Array {
                    path= TYPO3.TypoScript:ResourceUri {
                        path = ${path}
                    }
                    arg = '?h='
                    hash = ${Carbon.FileContent.pathHash(path)}
                }
            }

        }
    }
}
