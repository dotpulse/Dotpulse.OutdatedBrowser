prototype(Dotpulse.OutdatedBrowser:CSS) <  prototype(Neos.Fusion:Tag) {
    include = ${Configuration.setting('Dotpulse.OutdatedBrowser.includeCSS')}
    tagName = 'link'
    attributes {
        rel = 'stylesheet'
        href = Neos.Fusion:ResourceUri {
            path = 'resource://Dotpulse.OutdatedBrowser/Public/Main.css'
        }
    }
    @if.include = ${this.include}
}
