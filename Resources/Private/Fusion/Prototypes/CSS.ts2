// Namespaces are defined in the Root.fusion file

prototype(OB:CSS) <  prototype(Fusion:Tag) {
	@context.settings = ${Configuration.setting('Dotpulse.OutdatedBrowser')}
	tagName = 'link'
	attributes {
		rel = 'stylesheet'
		href = Fusion:ResourceUri {
			path = 'resource://Dotpulse.OutdatedBrowser/Public/Main.css'
		}
	}
	@if.includeCSS = ${settings.includeCSS}
	@position = 'end 5000'
}
