// Namespaces are defined in the Root.fusion file

prototype(OB:Javascript) <  prototype(Fusion:Tag) {
	@context.settings = ${Configuration.setting('Dotpulse.OutdatedBrowser')}
	tagName = 'script'
	attributes.src = Fusion:ResourceUri {
		path = 'resource://Dotpulse.OutdatedBrowser/Public/Main.js'
	}
	@if.includeJavascript = ${settings.includeJavascript}
}
